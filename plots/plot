#!/usr/local/bin/gnuplot

# ==========================================
#
#
#
#
# ==========================================

# Plot output settings
# --------------------

# PNG
set terminal png enhanced font './Vera.ttf,10'
set output "plot-11-30000.png"

# EPS
# - size (cm) is (2/3)*\linewidth x phi
# set terminal epslatex size 10.22,6.31 color colortext
# set output "test.tex"


# Labels and Axis
# ---------------

# set title 'tmanin runtimes for levels 11 to 30000'
set xlabel 'Level N'
set ylabel 'Time (seconds)'
# set logscale y
set key left top


# Plot
# ----

# Options for plotting include: points, lines, linespoints
plot "tmanin_runtimes-11-30000.dat" using 2 title 'Serial (Mirkwood)'


# Prevent flashing
# pause -1 "Hit any key to continue"

# Reset settings
# set term x11
